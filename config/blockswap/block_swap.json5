{
	// Generates all block states for all blocks in the registry.
	"generate_all_known_states": true,
	// Whether blocks are replaced in existing chunks.
	"retro_gen": true,
	/* A map of states that specifies what the "old" block state is and what its "new" block state is.
	   See "known_states" folder("generate_all_known_states" must be set to true in this config) to see all known block states available for all blocks available in the registry.
	   Example:
	    "state_swapper": [
	    	{
	    		"new": {
	    			"Name": "minecraft:birch_log",
	    			// Properties define the state of this block/fluid.
	    			"Properties": {
	    				"axis": "x"
	    			}
	    		},
	    		"old": {
	    			"Name": "minecraft:oak_log",
	    			// Properties define the state of this block/fluid.
	    			"Properties": {
	    				"axis": "z"
	    			}
	    		}
	    	},
	    	{
	    		"new": {
	    			"Name": "minecraft:birch_leaves",
	    			// Properties define the state of this block/fluid.
	    			"Properties": {
	    				"distance": "7",
	    				"persistent": "true"
	    			}
	    		},
	    		"old": {
	    			"Name": "minecraft:acacia_log",
	    			// Properties define the state of this block/fluid.
	    			"Properties": {
	    				"axis": "z"
	    			}
	    		}
	    	},
	    	{
	    		"new": {
	    			"Name": "minecraft:jungle_log",
	    			// Properties define the state of this block/fluid.
	    			"Properties": {
	    				"axis": "x"
	    			}
	    		},
	    		"old": {
	    			"Name": "minecraft:birch_log",
	    			// Properties define the state of this block/fluid.
	    			"Properties": {
	    				"axis": "z"
	    			}
	    		}
	    	},
	    	{
	    		"new": {
	    			"Name": "minecraft:jungle_planks",
	    		},
	    		"old": {
	    			"Name": "minecraft:acacia_planks",
	    			}
	    		}
	    	}
	    ]
	*/
	"state_swapper": [],
	/* A map of blocks that specifies what the "old" block is and what its "new" block is.
	   Example:
	   	"swapper": {
	   		"minecraft:coarse_dirt": "minecraft:dirt",
	   		"minecraft:diamond_block": "minecraft:emerald_block"
	   	}
	*/
	"swapper": {
		"ecologics:thin_ice": "minecraft:ice",
		"regions_unexplored:mossy_stone": "immersive_weathering:mossy_stone",
		"regions_unexplored:silt_dirt": "immersive_weathering:silt",
		"regions_unexplored:silt_grass_block": "immersive_weathering:grassy_silt",

		"biomesoplenty:palm_log": "regions_unexplored:palm_log",
		"biomesoplenty:palm_leaves": "regions_unexplored:palm_leaves",

		"biomesoplenty:dead_log": "regions_unexplored:dead_log",
		"biomesoplenty:dead_leaves": "regions_unexplored:dead_leaves",

		"regions_unexplored:mauve_log": "biomesoplenty:jacaranda_log",
		"regions_unexplored:mauve_leaves": "biomesoplenty:jacaranda_leaves",
		
		"regions_unexplored:silver_birch_log": "biomesoplenty:magic_log",
		"regions_unexplored:enchanted_birch_leaves": "biomesoplenty:magic_leaves",

		"regions_unexplored:ashen_log": "quark:ancient_log",
		"regions_unexplored:ashen_leaves": "quark:ancient_leaves",
		"regions_unexplored:ash": "biomesoplenty:black_sand",
		"regions_unexplored:ash_vent": "biomesoplenty:black_sand",
		"regions_unexplored:volcanic_ash": "biomesoplenty:black_sand",
		"regions_unexplored:ashen_dirt": "biomesoplenty:mossy_black_sand",
		
		"regions_unexplored:maple_log": "minecraft:spruce_log",
		"regions_unexplored:maple_leaves": "minecraft:spruce_leaves",
		
		"regions_unexplored:flowering_leaves": "biomesoplenty:flowering_oak_leaves",

		"regions_unexplored:barley": "biomesoplenty:barley"
	}
}